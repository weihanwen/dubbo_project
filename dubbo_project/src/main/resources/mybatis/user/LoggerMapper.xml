<?xml version="1.0" encoding="UTF-8"?>    
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="LoggerMapper">

    <insert id="saveLogger" parameterType="logger">
        insert into tb_logger(
          logger_id,path,requestmap,result,longtime,createtime
        )VALUES (
          #{logger_id},#{path},#{requestmap},#{result},#{longtime},now()
        )
     </insert>


</mapper> 